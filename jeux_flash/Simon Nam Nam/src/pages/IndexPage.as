package pages {	import pages.utils.KonamiCode;	import com.gaiaframework.api.ISound;	import com.gaiaframework.templates.AbstractPage;	import com.greensock.TweenMax;	import flash.events.Event;	public class IndexPage extends AbstractPage {				public var scoreDefini:Boolean;		public var score:int;		public var nom:String = "guest";		public function IndexPage() {			super();			alpha = 0;			//new Scaffold(this);		}		override public function transitionIn():void {			super.transitionIn();			TweenMax.to(this, 0.3, {alpha:1, onComplete:transitionInComplete});			var konamiCode:KonamiCode = new KonamiCode(stage);			konamiCode.addEventListener("KonamiCode", _konamiCode);		}		private function _konamiCode(evt:Event):void {						switch (uint(Math.random() * 3)){				
				case 0:
					ISound(assets.konami).play();
					break;
				case 1:					ISound(assets.canard).play();					break;									case 2:					ISound(assets.cordechasse).play();					break;
			}		}		override public function transitionOut():void {			super.transitionOut();			TweenMax.to(this, 0.3, {alpha:0, onComplete:transitionOutComplete});		}	}}